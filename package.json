{
  "name": "argent-project",
  "version": "1.0.0",
  "description": "\"Rehten's custom starter-kit\"",
  "main": "index.js",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --open --config webpack.developing.config.js",
    "start:prod": "cross-env NODE_ENV=production webpack --config webpack.production.config.js",
    "start:test": "webpack --config webpack.benchmarking.config.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/Sithas/Argent-Project.git"
  },
  "keywords": [
    "jest"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "author": "Rehten",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/Sithas/Argent-Project/issues"
  },
  "homepage": "https://gitlab.com/Sithas/Argent-Project#README",
  "devDependencies": {
    "@types/jest": "^22.1.1",
    "awesome-typescript-loader": "^3.4.1",
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "fork-ts-checker-webpack-plugin": "^0.3.0",
    "html-webpack-include-assets-plugin": "^1.0.4",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.9.0",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass-loader": "^7.0.1",
    "scss-loader": "0.0.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.3",
    "ts-jest": "^22.0.3",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.7.1",
    "url-loader": "^1.0.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "@types/bootstrap": "^4.1.0",
    "@types/react": "^16.0.36",
    "@types/react-bootstrap": "^0.32.2",
    "@types/react-dom": "^16.0.3",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "bootstrap": "^4.1.1",
    "hammer": "0.0.5",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0"
  }
